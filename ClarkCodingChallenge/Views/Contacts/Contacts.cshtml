@{
    ViewData["Title"] = "Contacts Page";
}


<div class="text-center">
    <table id="ContactsTable" class="table" cellspacing="1">
        <thead>
            <tr>
                <th>First Name:</th>
                <th>Last Name:</th>
                <th>Email:</th>
            </tr>

        </thead>
        <tbody>
            @foreach (var contact in Model) {
                <tr>
                    <td>@contact.FirstName</td>
                    <td>@contact.LastName</td>
                    <td>@contact.Email</td>
                </tr>
            }
        </tbody>
    </table>
</div>